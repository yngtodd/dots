    Hostname login1.ornl.gov
    User ygx

Host mochi
    User ygx
    ProxyCommand ssh ornl -W %h:%p
    ForwardAgent yes
    # Notebooks
    LocalForward 9980 localhost:9980
    LocalForward 9981 localhost:9981

Host dgx
    User ygx
    ProxyCommand ssh ornl -W dgx-dl04:22

Host super 
    Hostname home.ccs.ornl.gov
    User yngtodd

Host *
   AddKeysToAgent yes
   UseKeychain yes
   IdentityFile ~/.ssh/id_rsa

